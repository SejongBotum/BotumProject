<?xml version="1.0" encoding="UTF-8"?>
<!-- Sql Mapper -->
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="session">
	<select id="idCheck" resultType="Integer" parameterType="HashMap">
		select count(distinct id) from member where id=#{id} and pw=#{pw};
	</select>
	
	<select id="getMVO" resultType="MemberVO" parameterType="String">
		select * from member where id=#{paramMap.id};
	</select>
	
	<insert id="registerMVO" parameterType="MemberVO">
		insert into member(id, pw, name, is_professor) values(#{id}, #{pw}, #{name}, #{is_professor});
	</insert>
</mapper>